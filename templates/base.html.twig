<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" type="image/x-icon" href="{{ asset('uploads/images/favicon.png') }}">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body style="min-height: 100vh; background-color: #FFF" @click="open = !open">
        {{include ('partials/_home_header.html.twig')}}
        <div id="searchBar">{% block searchBar %}{% endblock %}</div>
        {% block body %}{% endblock %}
    </body>
</html>
